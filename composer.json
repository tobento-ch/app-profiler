{
    "name": "tobento/app-profiler",
    "type": "library",
    "description": "App profiler.",
    "keywords": [
        "tobento",
        "package",
        "app",
        "profiler",
        "tests"
	],
    "homepage": "https://www.tobento.ch",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Strub",
            "email": "info@tobento.ch",
            "homepage": "https://www.tobento.ch"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "tobento/app": "^1.0.7",
        "tobento/app-migration": "^1.0.3",
        "tobento/app-http": "^1.1.0",
        "tobento/app-view": "^1.0.2",
        "tobento/service-collection": "^1.0.7",
        "tobento/service-filesystem": "^1.0.5",
        "symfony/var-dumper": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.0",
        "tobento/app-testing": "^1.0",
        "tobento/app-translation": "^1.0",
        "tobento/app-database": "^1.0.2",
        "tobento/app-user": "^1.0",
        "tobento/app-console": "^1.0",
        "tobento/app-event": "^1.0.2",
        "tobento/app-queue": "^1.0.1",
        "tobento/app-logging": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Tobento\\App\\Profiler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tobento\\App\\Profiler\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --testdox --colors=always",
        "psalm": "vendor/bin/psalm"
    }
}